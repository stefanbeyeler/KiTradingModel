# MT5 Trade Agent Configuration
# Copy this file to .env and fill in your values

# Data Service URL
# Adjust to your network configuration
DATA_SERVICE_URL=http://10.1.19.101:3001

# Terminal Registration
# Get these values by running: python mt5_agent.py --register
TERMINAL_ID=
TERMINAL_API_KEY=

# Polling interval in seconds (default: 5)
# How often to check for new/changed trades
POLL_INTERVAL=5

# Heartbeat interval in seconds (default: 60)
# How often to send heartbeat to Data Service
HEARTBEAT_INTERVAL=60

# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# Optional: Log file path
# Leave empty for console-only logging
LOG_FILE=
